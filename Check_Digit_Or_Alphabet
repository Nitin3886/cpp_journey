#include<iostream>
using namespace std;
int main() {
    char a;

    cout << "Enter a character: ";
    cin >> a;
    if (isdigit(a)){
            cout<<a<<" is a digit." <<endl;
    }
    else if (isalpha(a)) {
        cout<<a<<" is an alphabet." <<endl;
    }
    else {
        cout<<a<<" is a special character." <<endl;
    }
    return 0;
}
